[
  {
    "toolchains": [
      {
        "repo": "https://gitlab.com/Panchajanya1999/azure-clang",
        "branch": "main",
        "name": "azure-clang",
        "binPath": ["bin"]
      }
    ],
    "params": {
     "ARCH": "arm64",
      "CC": "azure-clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "azure-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "azure-clang/bin/arm-linux-gnueabi-",
        "AR": "azure-clang/bin/llvm-ar",
        "NM": "azure-clang/bin/llvm-nm",
        "OBJCOPY": "azure-clang/bin/llvm-objcopy",
        "OBJDUMP": "azure-clang/bin/llvm-objdump",
        "READELF": "azure-clang/bin/llvm-readelf",
        "OBJSIZE": "azure-clang/bin/llvm-size",
        "STRIP": "azure-clang/bin/llvm-strip",
        "LLVM_AR": "azure-clang/bin/llvm-ar",
        "LLVM_DIS": "azure-clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    }
  }
]
